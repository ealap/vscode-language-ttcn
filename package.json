{
    "name": "language-ttcn",
    "displayName": "TTCN-3 Language Support for Visual Studio Code",
    "description": "Provides TTCN-3 language support for Visual Studio Code",
    "version": "0.14.0",
    "publisher": "ealap",
    "license": "MIT",
    "preview": true,
    "author": {
        "name": "Emmanuel Alap",
        "url": "https://github.com/ealap"
    },
    "engines": {
        "vscode": "^1.30.0"
    },
    "devDependencies": {
        "typescript": "^3.2.0",
        "vscode": "1.0.0"
    },
    "categories": [
        "Programming Languages",
        "Snippets"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/ealap/vscode-language-ttcn"
    },
    "keywords": [
        "ttcn-3",
        "ttcn3",
        "ttcn",
        "highlighter",
        "syntax",
        "snippet"
    ],
    "contributes": {
        "languages": [
            {
                "id": "ttcn",
                "aliases": [
                    "TTCN-3",
                    "ttcn-3",
                    "TTCN3",
                    "ttcn3",
                    "TTCN",
                    "ttcn"
                ],
                "extensions": [
                    ".ttcn3",
                    ".ttcn",
                    ".ttcnpp"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "ttcn",
                "scopeName": "source.ttcn",
                "path": "./syntaxes/ttcn.tmLanguage.json"
            }
        ],
        "snippets": [
            {
                "language": "ttcn",
                "path": "./snippets/ttcn.tmSnippet.json"
            }
        ]
    },
    "icon": "images/vscode-icon-ttcn.png",
    "__metadata": {
        "publisherDisplayName": "Emmanuel Alap"
    }
}